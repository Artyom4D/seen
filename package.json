    "swig"          : "^1.3.2",
    "marked"        : "^0.3.2",
    "highlight.js"  : "^8.0.0",
    "pngjs"         : "^0.4.0",
    "mocha"         : "^1.18.2",
    "phantomjs"     : "^1.9.7-1",
    "uglify-js"     : "^2.4.13",
    "chai"          : "^1.9.1",
    "q"             : "^1.0.1"
